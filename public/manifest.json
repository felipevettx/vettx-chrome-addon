{
  "manifest_version": 3,
  "name": "Vettx addon - React",
  "version": "1.5",
  "description": "Downloader of data from Facebook Marketplace make with react and tailwind",
  "permissions": [ 
  "activeTab",
  "storage",
  "tabs",
  "downloads",
  "scripting",
  "cookies",
  "windows"
  ],
  "host_permissions": [
    "https://*.facebook.com/*",
    "https://www.facebook.com/marketplace/*",
    "https://*.vettx.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html"
   
  },
  "content_scripts": [
    {
      "matches": ["https://*.facebook.com/*", "https://*.vettx.com/*"],
      "js": ["content.js"]
    }
  ]
 
}
